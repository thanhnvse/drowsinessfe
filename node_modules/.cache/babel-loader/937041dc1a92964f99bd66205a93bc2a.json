-flex flex-column\">\n              <itemGroup\n                v-for=\"(group, index) in display\"\n                v-bind:key=\"group.groupId\"\n                :group=\"group\"\n                :index=\"index\"\n              />\n            </v-row>\n          </v-card>\n          <v-row class=\"d-flex justify-center\">\n            <v-pagination\n              class=\"mt-5\"\n              v-model=\"page\"\n              :length=\"getTotalPage\"\n              :total-visible=\"7\"\n              prev-icon=\"mdi-menu-left\"\n              next-icon=\"mdi-menu-right\"\n              color=\"#727cf5\"\n              v-if=\"groups.length > 0\"\n            ></v-pagination>\n            <span class=\"d-flex flex-column align-center cursor topview-lable mt-16\" v-else>\n              Bạn chưa tạo khu trọ nào để quản lý\n            </span>\n          </v-row>\n        </v-col>\n      </v-row>\n    </v-container>\n    <v-container v-if=\"!isLoading\" class=\"pa-0 hidden-sm-and-up\">\n      <v-row justify=\"center\" class=\"ma-0 pa-0\">\n        <v-col cols=\"12\" class=\"d-flex align-center py-0\">\n          <span class=\"page-mobile-title\">Bạn đang quản lý {{ groups.length }} khu trọ</span>\n          <v-btn\n            class=\"ml-auto btn-success btn-sm font-nunito white--text\"\n            @click=\"openCreateGroupMobileDialog = true\"\n            ><v-icon small class=\"mr-1\">mdi mdi-plus</v-icon>Thêm khu trọ mới</v-btn\n          >\n        </v-col>\n        <v-col cols=\"12\" class=\"d-flex align-center\">\n          <v-text-field\n            v-show=\"showSearchGroup\"\n            label=\"Tìm theo tên khu trọ\"\n            v-model=\"searchGroupQuery\"\n            solo\n            hide-details\n            class=\"text-muted py-1 size-sub-2 light-text-field font-nunito\"\n            clearable\n            @input=\"changeSearchQuery\"\n            style=\"border-top-right-radius: 0px; border-bottom-right-radius: 0px\"\n          ></v-text-field>\n       