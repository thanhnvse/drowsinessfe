.doc = doc;\n    },\n    closeChatBox() {\n      this.chatBox.show = false;\n    },\n  },\n  async created() {\n    this.getGroups().then(() => {\n      this.getTypes();\n    });\n    this.getUser();\n  },\n};\n</script>\n"],"sourceRoot":"src/views/vendor"}]}