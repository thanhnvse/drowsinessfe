A,GALA,EAKA;AACA,UAAA,KAAA,GAAA,KAAA,WAAA,CAAA,SAAA,CAAA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,WAAA,KAAA,GAAA;AAAA,OAAA,CAAA;;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,EAAA;AACA,aAAA,WAAA,gCACA,KAAA,WAAA,CAAA,KAAA,CAAA,CAAA,EAAA,KAAA,CADA,sBAEA,KAAA,WAAA,CAAA,KAAA,CAAA,KAAA,GAAA,CAAA,CAFA;AAIA,aAAA,KAAA,CAAA,WAAA,EAAA,KAAA,WAAA;AACA;AACA,KAdA;AAeA,IAAA,qBAfA,mCAeA;AAAA;;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA;AAAA,eAAA,KAAA,CAAA,KAAA,CAAA,UAAA,CAAA,KAAA,EAAA;AAAA,OAAA;AACA,KAlBA;AAmBA,IAAA,YAnBA,wBAmBA,CAnBA,EAmBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,MAAA,CAAA,KAAA;AADA,UAEA,KAFA,GAEA,CAAA,CAAA,MAFA,CAEA,KAFA;AAGA,WAAA,KAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,SAAA,GAAA,KAAA,CAAA,IAAA,CAAA,KAAA,EAAA,GAAA,CAAA,UAAA,IAAA;AAAA,eAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA;AAAA,OAAA,CAAA;AACA,KAxBA;AAyBA,IAAA,YAzBA,0BAyBA;AAAA;;AACA,UAAA,EAAA,GAAA,IAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,KAAA,EAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,EAAA,CAAA,MAAA,CAAA,OAAA,EAAA,IAAA,EAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,WAAA,UAAA,CAAA,EAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,SAAA,GAAA,MAAA,CAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,GAAA;AAAA,iBAAA;AAAA,YAAA,WAAA,EAAA;AAAA,WAAA;AAAA,SAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,gCAAA,MAAA,CAAA,WAAA,sBAAA,MAAA,CAAA,MAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,WAAA,EAAA,MAAA,CAAA,WAAA;;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,YAAA,CAAA,wBAAA,EAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SAAA;AACA,OAPA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,QAAA,MAAA,CAAA,YAAA,CAAA,sBAAA,EAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SAAA;AACA,OAXA;AAYA;AA1CA,GAxBA;AAoEA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA;AACA,MAAA,OADA,qBACA,CACA;AACA,OAHA;AAIA,MAAA,IAAA,EAAA;AAJA;AADA,GApEA;AA4EA,EAAA,OA5EA,qBA4EA;AACA,SAAA,WAAA,GAAA,KAAA,SAAA;AACA;AA9EA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-dialog v-model=\"dialog.showCurrImg\">\n      <v-card max-height=\"500px\">\n        <v-img :src=\"dialog.currImg\"></v-img>\n      </v-card>\n    </v-dialog>\n    <v-dialog v-model=\"dialog.show\" width=\"350\">\n      <v-card height=\"350\" :loading=\"isFileUploading\">\n        <div class=\"d-flex flex-column align-center justify-center\">\n          <input\n            type=\"file\"\n            @change=\"onFileChange\"\n            ref=\"fileSelect\"\n            multiple\n            lang=\"vi\"\n            accept=\"image/*\"\n            class=\"ma-2\"\n          />\n          <div class=\"d-flex flex-wrap\" style=\"height: 250px; overflow-y: auto\">\n            <v-img\n              v-for=\"url in upload.imageUrls\"\n              :key=\"url\"\n              :src=\"url\"\n              height=\"100\"\n              width=\"100\"\n              class=\"ma-1 elevation-2\"\n            ></v-img>\n          </div>\n        </div>\n        <v-card-actions>\n          <v-spacer />\n          <v-btn color=\"#727CF5\" v-if=\"this.upload.imageUrls.length > 0\" @click=\"uploadImages\" dark>\n            <v-icon left>cloud_upload</v-icon> Tải lên\n          </v-btn>\n          <v-spacer />\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n    <v-row>\n      <v-col v-if=\"mode === 'edit'\" cols=\"12\" :class=\"check === 1 ? 'd-flex justify-center' : ''\">\n        <v-btn class=\"ml-2\" @click=\"openImageUploadDialog\" depressed>\n          <v-icon left>add_photo_alternate</v-icon>Tải lên ảnh mới</v-btn\n        >\n      </v-col>\n      <v-col cols=\"12\">\n        <div style=\"max-height: 300px; overflow-y: auto\">\n          <!-- display input images -->\n          <!-- if mode === view -->\n          <div class=\"d-flex flex-row flex-wrap align-start justify-start\">\n            <v-img\n              v-for=\"image in mergeImages\"\n              :key=\"image.imageId\"\n              :src=\"image.resourceUrl\"\n              :lazy-src=\"image.resourceUrl\"\n              aspect-ratio=\"1\"\n              class=\"grey lighten-2 ma-2\"\n              height=\"120\"\n              width=\"120\"\n              @click=\"showCurrImg(image.resourceUrl)\"\n            >\n              <template v-slot:placeholder>\n                <v-row class=\"fill-height ma-0\" align=\"center\" justify=\"center\">\n                  <v-progress-circular indeterminate color=\"grey lighten-5\"></v-progress-circular>\n                </v-row>\n              </template>\n              <v-row v-if=\"mode === 'edit'\" class=\"fill-height ma-0\" align=\"center\" justify=\"center\">\n                <v-btn icon depressed color=\"blue-grey\" @click=\"removeImage(image.resourceUrl)\">\n                  <v-icon color=\"red\">delete_forever</v-icon>\n                </v-btn>\n              </v-row>\n            </v-img>\n          </div>\n          <!-- display uploaded images -->\n        </div>\n      </v-col>\n    </v-row>\n\n    <v-snackbar\n      v-model=\"snackBarMixin.show\"\n      :multi-line=\"snackBarMixin.multiLine\"\n      :timeout=\"snackBarMixin.timeout\"\n      :absolute=\"snackBarMixin.absolute\"\n      :color=\"snackBarMixin.color\"\n    >\n      {{ snackBarMixin.message }}\n\n      <template v-slot:action=\"{ attrs }\">\n        <v-btn color=\"red\" text v-bind=\"attrs\" @click=\"snackBarMixin.show = false\"> Close </v-btn>\n      </template>\n    </v-snackbar>\n  </div>\n</template>\n<script>\nimport fileMixins from '../../mixins/file';\nimport snackBarMixins from '../../mixins/snackBar';\n\nexport default {\n  name: 'ImageEditor',\n  props: {\n    oldImages: {},\n    mode: {\n      default: 'edit',\n      validator: (value) => ['edit', 'view'].includes(value.toLowerCase()),\n    },\n    check: {},\n  },\n  mixins: [fileMixins, snackBarMixins],\n  data: () => ({\n    dialog: {\n      show: false,\n      files: null,\n      currImg: null,\n      showCurrImg: false,\n    },\n    upload: {\n      imageUrls: [],\n      locals: [],\n    },\n    mergeImages: [],\n  }),\n  methods: {\n    showCurrImg(url) {\n      this.dialog.currImg = url;\n      this.dialog.showCurrImg = true;\n    },\n    removeImage(url) {\n      const index = this.mergeImages.findIndex((img) => img.resourceUrl === url);\n      if (index > -1) {\n        this.mergeImages = [\n          ...this.mergeImages.slice(0, index),\n          ...this.mergeImages.slice(index + 1),\n        ];\n        this.$emit('newValues', this.mergeImages);\n      }\n    },\n    openImageUploadDialog() {\n      this.dialog.show = true;\n      this.$nextTick(() => this.$refs.fileSelect.click());\n    },\n    onFileChange(e) {\n      console.log(e.target.files);\n      const { files } = e.target;\n      this.files = files;\n      this.upload.imageUrls = Array.from(files).map((file) => URL.createObjectURL(file));\n    },\n    uploadImages() {\n      const fd = new FormData();\n      Array.from(this.files).forEach((file) => {\n        fd.append('files', file, file.data);\n      });\n      this.uploadFile(fd)\n        .then(() => {\n          this.upload.imageUrls = this.listUploadedFiles.map((url) => ({ resourceUrl: url }));\n          this.mergeImages = [...this.mergeImages, ...this.upload.imageUrls];\n          this.$emit('newValues', this.mergeImages);\n          this.dialog.show = false;\n          this.showSnackBar('Tải ảnh lên thành công', { color: 'green' });\n        })\n        .catch((error) => {\n          console.log(error);\n          this.showSnackBar('Tải ảnh lên thất bại', { color: 'red' });\n        });\n    },\n  },\n  watch: {\n    upload: {\n      handler() {\n        // this.$emit('newValues', this.mergeImages);\n      },\n      deep: true,\n    },\n  },\n  created() {\n    this.mergeImages = this.oldImages;\n  },\n};\n</script>\n"],"sourceRoot":"src/components/vendor/hostel_management"}]}