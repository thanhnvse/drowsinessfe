A,QAAA,KAAA,EAAA,KADA;AAEA,QAAA,MAAA,EAAA,MAFA;AAGA,QAAA,KAAA,EAAA,KAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAAA;AAMA,KA7BA;AA+BA,IAAA,UA/BA,wBA+BA;AACA,aAAA,KAAA,IAAA,CAAA,UAAA;AACA;AAjCA,GAJA;AAwCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AAAA;;AACA;AACA;AACA,WAAA,UAAA,GAAA,KAAA,IAAA,CAAA,MAAA,CAAA,KAAA,gBAAA,EAAA,CAAA;AACA,WAAA,UAAA,CACA,IADA,CACA;AAAA,eAAA,KAAA,CAAA,KAAA,CAAA,UAAA,EAAA,KAAA,CAAA,IAAA,CAAA;AAAA,OADA,EAEA,IAFA,CAEA;AAAA,eAAA,GAAA,gBAAA,KAAA,CAAA,UAAA,eAAA;AAAA,OAFA;AAGA,KARA;AAUA,IAAA,WAVA,uBAUA,IAVA,EAUA;AACA,UAAA,CAAA,IAAA,EAAA,OADA,CAGA;AACA;AACA;;AACA,MAAA,IAAA,CAAA,QAAA,GANA,CAQA;AACA;;;AACA,UAAA,KAAA,UAAA,EAAA,KAAA,UAAA,CAAA,MAAA;AACA,KArBA;AAuBA,IAAA,gBAvBA,8BAuBA;AAAA,UACA,QADA,GACA,IADA,CACA,QADA;AAEA,UAAA,aAAA,GAAA,KAAA,GAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AAEA,aAAA;AAAA,QAAA,aAAA,EAAA,aAAA;AAAA,QAAA,QAAA,EAAA;AAAA,OAAA;AACA;AA5BA,GAxCA;AAuEA,EAAA,KAAA,EAAA;AACA,IAAA,IADA,gBACA,KADA,EACA,OADA,EACA;AACA,WAAA,WAAA,CAAA,OAAA;AACA;AAHA,GAvEA;AA6EA,EAAA,OA7EA,qBA6EA;AACA;AACA;AACA,SAAA,QAAA,GAAA,KAAA,IAAA,CAAA,WAAA,CAAA,KAAA,KAAA,CAAA;AACA,GAjFA;AAmFA,EAAA,OAnFA,qBAmFA;AACA,IAAA,GAAA,gBAAA,KAAA,UAAA,cAAA;AACA,SAAA,UAAA;AACA,GAtFA;AAwFA,EAAA,aAxFA,2BAwFA;AACA,SAAA,WAAA,CAAA,KAAA,IAAA;AACA,GA1FA;AA4FA,EAAA,MA5FA,kBA4FA,CA5FA,EA4FA;AAAA,QACA,KADA,GACA,IADA,CACA,WADA;AAEA,WAAA,CAAA,CAAA,QAAA,EAAA;AAAA,MAAA,KAAA,EAAA;AAAA,KAAA,CAAA;AACA;AA/FA,CAAA","sourcesContent":["/* eslint-disable */\n<script>\nimport debug from 'debug';\n\nconst log = debug('app:vue_features/documents/components/PDFPage');\n\nexport default {\n  name: 'PDFPage',\n  props: ['page', 'scale'],\n\n  computed: {\n    actualSizeViewport() {\n      return this.viewport.clone({ scale: this.scale });\n    },\n\n    canvasStyle() {\n      const { width: actualSizeWidth, height: actualSizeHeight } = this.actualSizeViewport;\n      const pixelRatio = window.devicePixelRatio || 1;\n      const [pixelWidth, pixelHeight] = [actualSizeWidth, actualSizeHeight].map(\n        (dim) =>\n          // eslint-disable-next-line\n          Math.ceil(dim / pixelRatio),\r\n        // eslint-disable-next-line\n      );\r\n      return `width: ${pixelWidth}px; height: ${pixelHeight}px;`;\n    },\n\n    canvasAttrs() {\n      let { width, height } = this.viewport;\n      [width, height] = [width, height].map((dim) => Math.ceil(dim));\n\n      const style = this.canvasStyle;\n\n      return {\n        width,\n        height,\n        style,\n        class: 'pdf-page',\n      };\n    },\n\n    pageNumber() {\n      return this.page.pageNumber;\n    },\n  },\n\n  methods: {\n    renderPage() {\n      // PDFPageProxy#render\n      // https://mozilla.github.io/pdf.js/api/draft/PDFPageProxy.html\n      this.renderTask = this.page.render(this.getRenderContext());\n      this.renderTask\n        .then(() => this.$emit('rendered', this.page))\n        .then(() => log(`Page ${this.pageNumber} rendered`));\n    },\n\n    destroyPage(page) {\n      if (!page) return;\n\n      // PDFPageProxy#_destroy\n      // https://mozilla.github.io/pdf.js/api/draft/PDFPageProxy.html\n      // eslint-disable-next-line\n      page._destroy();\r\n\n      // RenderTask#cancel\n      // https://mozilla.github.io/pdf.js/api/draft/RenderTask.html\n      if (this.renderTask) this.renderTask.cancel();\n    },\n\n    getRenderContext() {\n      const { viewport } = this;\n      const canvasContext = this.$el.getContext('2d');\n\n      return { canvasContext, viewport };\n    },\n  },\n\n  watch: {\n    page(page, oldPage) {\n      this.destroyPage(oldPage);\n    },\n  },\n\n  created() {\n    // PDFPageProxy#getViewport\n    // https://mozilla.github.io/pdf.js/api/draft/PDFPageProxy.html\n    this.viewport = this.page.getViewport(this.scale);\n  },\n\n  mounted() {\n    log(`Page ${this.pageNumber} mounted`);\n    this.renderPage();\n  },\n\n  beforeDestroy() {\n    this.destroyPage(this.page);\n  },\n\n  render(h) {\n    const { canvasAttrs: attrs } = this;\n    return h('canvas', { attrs });\n  },\n};\n</script>\n<style>\n.pdf-page {\n  display: block;\n  margin: 0 auto 0.5em;\n}\n</style>\n"],"sourceRoot":"src/components/vendor/pdfviewer"}]}