         <v-icon>format_bold</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.italic() }\"\n            @click=\"commands.italic\"\n          >\n            <v-icon>format_italic</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.strike() }\"\n            @click=\"commands.strike\"\n          >\n            <v-icon>format_strikethrough</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.underline() }\"\n            @click=\"commands.underline\"\n          >\n            <v-icon>format_underline</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.code() }\"\n            @click=\"commands.code\"\n          >\n            <v-icon>code</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.paragraph() }\"\n            @click=\"commands.paragraph\"\n          >\n            <v-icon>fas fa-paragraph</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.heading({ level: 1 }) }\"\n            @click=\"commands.heading({ level: 1 })\"\n          >\n            H1\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.heading({ level: 2 }) }\"\n            @click=\"commands.heading({ level: 2 })\"\n          >\n            H2\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.heading({ level: 3 }) }\"\n            @click=\"commands.heading({ level: 3 })\"\n          >\n            H3\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.bullet_list() }\"\n            @click=\"commands.bullet_list\"\n          >\n            <v-icon>format_list_bulleted</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.ordered_list() }\"\n            @click=\"commands.ordered_list\"\n          >\n            <v-icon>format_list_numbered</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.blockquote() }\"\n            @click=\"commands.blockquote\"\n          >\n            <v-icon>format_quote</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"menubar__v-btn\"\n            :class=\"{ 'is-active': isActive.code_block() }\"\n            @click=\"commands.code_block\"\n          >\n            <v-icon>code</v-icon>\n          </v-btn>\n\n          <v-btn icon class=\"menubar__v-btn\" @click=\"commands.horizontal_rule\">\n            <v-icon>horizontal_rule</v-icon>\n          </v-btn>\n\n          <v-btn icon class=\"menubar__v-btn\" @click=\"commands.undo\">\n            <v-icon>undo</v-icon>\n          </v-btn>\n\n          <v-btn icon class=\"menubar__v-btn\" @click=\"commands.redo\">\n            <v-icon>redo</v-icon>\n          </v-btn>\n        </div>\n      </editor-menu-bar>\n      <!-- <v-switch\n        v-model=\"isUsingTemplate\"\n        :label=\"`Phụ lục hợp đồng mẫu`\"\n        class=\"filter\"\n        color=\"#727CF5\"\n      ></v-switch> -->\n      <v-divider class=\"my-2\" style=\"background-color: black\" />\n      <editor-content class=\"editor__content font-weight-regular\" :editor=\"editor\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Editor, EditorContent, EditorMenuBar } from 'tiptap';\nimport {\n  Blockquote,\n  CodeBlock,\n  HardBreak,\n  Heading,\n  HorizontalRule,\n  OrderedList,\n  BulletList,\n  ListItem,\n  TodoItem,\n  TodoList,\n  Bold,\n  Code,\n  Italic,\n  Link,\n  Strike,\n  Underline,\n  History,\n  Placeholder,\n} from 'tiptap-extensions';\n\nexport default {\n  name: 'TextEditor',\n  components: {\n    EditorContent,\n    EditorMenuBar,\n  },\n  props: ['editorContent', 'template'],\n  computed: {\n    htmlContent() {\n      if (this.editor) {\n        return this.editor.getHTML();\n      }\n      return '';\n    },\n  },\n  methods: {\n    setEditorContent() {\n      return new Editor({\n        extensions: [\n          new Blockquote(),\n          new BulletList(),\n          new CodeBlock(),\n          new HardBreak(),\n          new Heading({ levels: [1, 2, 3] }),\n          new HorizontalRule(),\n          new ListItem(),\n          new OrderedList(),\n          new TodoItem(),\n          new TodoList(),\n          new Link(),\n          new Bold(),\n          new Code(),\n          new Italic(),\n          new Strike(),\n          new Underline(),\n          new History(),\n          new Placeholder({\n            emptyEditorClass: 'is-editor-empty',\n            emptyNodeClass: 'is-empty',\n            emptyNodeText: 'Phụ lục hợp đồng...',\n            showOnlyWhenEditable: true,\n            showOnlyCurrent: true,\n          }),\n        ],\n        content: `\n          <span></span>\n        `,\n      });\n    },\n  },\n  data() {\n    return {\n      editor: null,\n      isLoadingTemplate: false,\n      isUsingTemplate: false,\n      initContent: null,\n    };\n  },\n  created() {\n    this.editor = this.setEditorContent();\n    if (this.editorContent) {\n      this.editor.setContent(this.editorContent);\n      this.initContent = this.editorContent;\n    } else {\n      this.editor.setContent(this.template);\n    }\n  },\n  beforeDestroy() {\n    this.editor.destroy();\n  },\n  watch: {\n    htmlContent: {\n      handler() {\n        this.$emit('appendixContent', this.htmlContent);\n      },\n    },\n    isUsingTemplate: {\n      handler(using) {\n        if (using) {\n          this.editor.setContent(this.template);\n        } else {\n          this.editor.setContent(this.editorContent);\n        }\n      },\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n.editor p.is-editor-empty:first-child::before {\n  content: attr(data-empty-text);\n  float: left;\n  color: #aaa;\n  pointer-events: none;\n  height: 0;\n  font-style: italic;\n}\n</style>\n"],"sourceRoot":"src/components/vendor/contract"}]}